{
    "presets": [
        ["env", {
            "modules": false,
            "useBuiltIns": true,
            "targets": {
                "node": "6.9"
            }
        }],
        "react"
    ],
    "plugins": [
        ["transform-object-rest-spread", {
            "useBuiltIns": true
        }],
        "transform-class-properties"
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    }
}
